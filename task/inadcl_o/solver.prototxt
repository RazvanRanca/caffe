net: "task/inadcl_o/train_val.prototxt"
test_iter: 59
test_interval: 200
test_compute_loss: 1
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 1000
display: 10
max_iter: 3000
momentum: 0.9
weight_decay: 0.0005
snapshot: 3000
snapshot_prefix: "task/inadcl_o/none/"
solver_mode: GPU
